  "scripts": {
    "test": "npm run stylelint && npm run eslint && npm run ava",
    "test-pr": "npm run stylelint-pr && npm run eslint-pr && npm run ava",
    "ava": "CONFIG=$(pwd)/webpack-ava.config.js BABEL_DISABLE_CACHE=1 NODE_ENV=AVA NODE_PATH=./app ava --verbose",
    "eslint": "eslint --ext .js,.jsx ./app ./config ./server ./utils",
    "eslint-pr": "eslint --ext .js,.jsx `git diff --name-status $(git merge-base origin/develop HEAD)..HEAD | grep -E '^[AM]' | awk ' {print $2} ' | grep -E 'jsx?$'`",
    "stylelint": "stylelint app/components/**/*.css",
    "stylelint-pr": "stylelint `git diff --name-status $(git merge-base origin/develop HEAD)..HEAD | grep -E '^[AM]' | awk ' {print $2} ' | grep -E 'css?$'`",
    "build": "webpack --config webpack-production.config.js --progress --profile --colors",
    "start": "nodemon --watch server/ --exec node ./server",
    "server": "node ./server",
    "sitemap": "node ./utils/sitemap",
    "production": "npm run build && npm run sitemap && npm run server",
    "storybook": "start-storybook -p 9001",
    "storybook-build": "build-storybook -o static/components"
  },
  "ava": {
    "require": "babel-register",
    "babel": "inherit"
  },
